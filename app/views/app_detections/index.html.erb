<p id="notice"><%= notice %></p>

<h1>App Detections</h1>

<table>
  <thead>
    <tr>
      <th>App type</th>
      <th>Strategy</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_detections.each do |app_detection| %>
      <tr>
        <td><%= app_detection.app_type %></td>
        <td><%= app_detection.strategy %></td>
        <td><%= link_to 'Show', app_detection %></td>
        <td><%= link_to 'Edit', edit_app_detection_path(app_detection) %></td>
        <td><%= link_to 'Destroy', app_detection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Detection', new_app_detection_path %>
