<p id="notice"><%= notice %></p>

<h1>App Detection Strategies</h1>

<table>
  <thead>
    <tr>
      <th>App detection</th>
      <th>Implementation in ruby dsl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_detection_strategies.each do |app_detection_strategy| %>
      <tr>
        <td><%= app_detection_strategy.app_detection_id %></td>
        <td><%= app_detection_strategy.implementation_in_ruby_dsl %></td>
        <td><%= link_to 'Show', app_detection_strategy %></td>
        <td><%= link_to 'Edit', edit_app_detection_strategy_path(app_detection_strategy) %></td>
        <td><%= link_to 'Destroy', app_detection_strategy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Detection Strategy', new_app_detection_strategy_path %>
